{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "plugins": [
        "react",
        "mocha"
    ],
    "extends": "airbnb",
    "rules": {
        "max-len": [1, 150],
        "indent": ["error", 2],
        "no-console": 0,
        "new-cap": 1,
        "strict": 0,
        "mocha/no-exclusive-tests": 2
    }
}
